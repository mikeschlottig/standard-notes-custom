{
  "name": "standard-notes-enhanced-editor",
  "version": "1.0.0",
  "description": "Enhanced Standard Notes editor with Mermaid and Excalidraw integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "standard-notes",
    "editor",
    "mermaid",
    "excalidraw",
    "diagrams"
  ],
  "author": "Mike Schlottig",
  "license": "MIT",
  "dependencies": {
    "mermaid": "^10.6.1",
    "@excalidraw/excalidraw": "^0.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.50.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  }
}